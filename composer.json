{
    "name": "hugojf/model-warnings",
    "description": "Laravel model warnings",
    "keywords": [
        "hugojf",
        "model-warnings"
    ],
    "homepage": "https://github.com/hugojf/model-warnings",
    "license": "GPL-3.0-only",
    "type": "library",
    "authors": [
        {
            "name": "Hugo Jeller"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5.8.0|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "php-coveralls/php-coveralls": "^2.2",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "HugoJF\\ModelWarnings\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HugoJF\\ModelWarnings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
